{
  "name": "kanelbulle-react-intl",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "html-react-parser": "^0.6.1",
    "react-i18n-jed": "^3.1.6",
    "po2json": "^1.0.0-alpha",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-intl": "^2.8.0",
    "react-intl-cra": "^0.3.4",
    "react-intl-translations-manager": "^5.0.3",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "trans:parse": "echo '\n' && react-intl-cra 'src/**/*.{js,jsx}' -o 'src/translations/messages/messages.json'",
    "trans:merge": "echo '\n' && node ./translationRunner.js",
    "trans:push": "echo '\n' && node ./scripts/sendTranslations.js",
    "trans:pull": "echo '\n' && node ./scripts/translate.js",
    "trans:late": "echo '\n' && node ./scripts/translate.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
