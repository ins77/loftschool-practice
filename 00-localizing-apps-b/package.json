{
  "name": "fikajs-gettext",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "html-react-parser": "^0.6.1",
    "po2json": "^1.0.0-alpha",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-i18n-jed": "^3.1.6",
    "react-intl": "^2.8.0",
    "react-intl-cra": "^0.3.4",
    "react-intl-translations-manager": "^5.0.3",
    "react-scripts": "2.1.3"
  },
  "devDependencies": {
    "react-gettext-parser": "^1.7.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "trans:parse": "echo '\n' && react-gettext-parser --trim-lines --no-wrap --output messages.pot --config gettext-parser.js  'src/**/{*.js,*.jsx,*.ts,*.tsx}' '!src/**/{*.spec.*}'",
    "trans:merge": "echo '\n' && node ./scripts/mergeTranslations.js -- no-warning",
    "trans:push": "echo '\n' && node ./scripts/pushTranslations.js -- no-warning",
    "trans:late": "echo '\n' && node ./scripts/translateFiles.js -- no-warning",
    "trans:pull": "echo '\n' && node ./scripts/translateFiles.js -- no-warning",
    "trans:json": "echo '\n' && node ./scripts/generateJson.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
